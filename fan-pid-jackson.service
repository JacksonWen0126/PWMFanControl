[Unit]
Description=Fan PID control Service
DefaultDependencies=no
StartLimitIntervalSec=60
[Service]
RootDirectory=/
User=root
Type=forking
ExecStart=/bin/bash -c '/usr/bin/python3 /usr/local/bin/FanPIDUbuntu.py &'
RemainAfterExit=yes
Restart=always
RestartSec=30
[Install]
WantedBy=multi-user.target
